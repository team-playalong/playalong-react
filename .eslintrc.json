{
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "arrow-body-style": ["error", "always"],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/*.test.js", "**/*.spec.js"]}]
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "jasmine"
    ],
    "env": {
      "jasmine": true
    }
}
